<div id="giphy_result" class="col-md-6 col-sm-6 col-xs-6 col-lg-6 giphys">
<p>Click the gif you want to use for your activity image!</p>
<% @urls.each_with_index do |url, index| image_tag url %>
<div class="activity-image container" style="background: url(<%= url %>) 50% 50% no-repeat;background-size: cover;">
</div>
<% end%>
</div>
