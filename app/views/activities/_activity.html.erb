
<div class="col-md-4 col-xs-12">
  <div class="">
    <%= image_tag(activity.image_url) %>
  </div>
  <div class="">
    <%= activity.name %>
  </div>
  <ul>
    <% activity.tags.each do |tag| %>
      <li><%= tag.name %> </li>
    <% end %>
  </ul>
  <%= image_tag(activity.host.profile.image_url, class: "col-md-3 col-xs-3") %>

</div>
